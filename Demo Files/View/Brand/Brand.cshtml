            $.post(url = "/Itempage/RemoveItemFromWishList", { ProductID: pid.toString() }, function (data) {
                $('#wishlistCount').html(data.WishCount.toString());
                $('#wish_' + pid.toString()).removeClass('active');
                $('#wish_' + pid.toString() + ' i').removeClass('fa-heart');
                $('#wish_' + pid.toString() + ' i').addClass('fa-heart-o');
                $('#wish_' + pid.toString()).attr('onclick', 'Addtowishlist(' + pid.toString() + ');');
            });
        }

        $(document).ready(function () {
            @ViewBag
        });
    </script>

</div>
