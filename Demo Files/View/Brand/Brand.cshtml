                //$('body').append('<div class="modal-backdrop fade in" style="opacity:0.5;background-color:#eee;text-align:center;width:100%;"><div class="loader"><span></span><span></span><span></span></div></div>');
                $('body').append('<div class="modal-backdrop fade in" style="opacity:0.5;background-color:#eee;text-align:center;width:100%;"><img src="/images/ajaxloader.gif" style="position:absolute;top:50%;" /></div>');

                $.post(url = "/Brand/RemoveCookie", { productIds: pid }, function (data) {
                    $('#comparecount').html(data.comaprecount);
                    $('#aproductcomapre').removeAttr('href');
                    if (data.comaprecount == "0") {
                        $('#aproductcomapre').attr('href', 'javascript:void(0);');
                    }
                    else {
                        $('#aproductcomapre').attr('href', '/itempage/Productcomapre');
                    }
                    $('.modal-backdrop').remove();
                });
            }

