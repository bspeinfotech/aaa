        function getdatabysearch() {

            MakeStringOfSelectedFilterForUrl(0);   // 0 means no load more

            manufacturerId = '@Request.QueryString["manufacturerId"].ToString()';

            var facetData = {
                "manufacturerId": manufacturerId,
                "url": $('#hdnurl').val(),
                "urltabid": $('#hdntabid').val(),
                "guidenew": $('#hdnguid').val(),
                "isAvailablePrescription": isPrescription,
                "isPolarized": isPolarized,
                "isInStock": isInStock,
                "sortby": sortby,
                "gender": allgender,
                "category": allCategory,
                "frameType": allFrameType,
                "frameShape": allFrameShape,
                "lensColor": allLensColor,
                "frameColor": allFrameColor,
            };

            $.ajax({
                type: "POST",
                contentType: "application/json; charset=utf-8",
                url: "/Brand/GetAllfilteredData",
                datatype: "json",
