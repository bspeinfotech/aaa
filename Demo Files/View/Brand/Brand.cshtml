        function Shorting() {

            $('body').append('<div class="modal-backdrop fade in" style="opacity:0.5;background-color:#eee;text-align:center;width:100%;"><img src="/images/ajaxloader.gif" style="position:absolute;top:50%;" /></div>');

            BindUrl(0);

            sortby = $("#ddlSorting").val();

            var facetData = {
                "Sortby": sortby,
                "manufactureId": '@Request.QueryString["manufacturerId"].ToString()'
            };

            $.ajax({
                type: "POST",
                contentType: "application/json; charset=utf-8",
                url: "/Brand/ShortingProductsAndBinding",
                datatype: "json",
                data: JSON.stringify(facetData),
                success: function (data) {

                    $("#div1").html(data);
                    $("#divgridlist").html(data);

                    if ($('#div1').css('display') == 'block') {
                        $("#divgridlist").hide();
                        $("#div1").show();
                    }
                    else {
                        $("#divgridlist").show();
                        $("#div1").hide();
                    }

                    $('.modal-backdrop').remove();
                }
            });
        }

